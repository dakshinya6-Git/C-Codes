#include <stdio.h>

int p(int a[], int l, int r){
    int pv=a[r], i=l-1, j, t;
    for(j=l;j<r;j++){
        if(a[j]<pv){
            i++;
            t=a[i]; a[i]=a[j]; a[j]=t;
        }
    }
    t=a[i+1]; a[i+1]=a[r]; a[r]=t;
    return i+1;
}

void qs(int a[], int l, int r){
    if(l<r){
        int pi=p(a,l,r);
        qs(a,l,pi-1);
        qs(a,pi+1,r);
    }
}

int main(){
    int n,i;
    printf("Enter n: ");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    qs(a,0,n-1);
    printf("Sorted: ");
    for(i=0;i<n;i++)
        printf("%d ",a[i]);
    return 0;
}
